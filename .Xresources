URxvt*depth: 32
URxvt*dynamicColors: on
URxvt*scrollBar: false
URxvt*cursorBlink: true
URxvt*url-launcher: /usr/bin/chromium
URxvt*font: xft:Droid Sans Mono:size=10
URxvt*boldFont: xft:Droid Sans Mono:bold:size=10
URxvt*italicFont: xft:Droid Sans Mono:italic:size=10
URxvt*bolditalicFont: xft:Droid Sans Mono:bold italic:size=color10

Xft.antialias: 1
Xft.dpi: 96
Xft.lcdfilter: lcddefault

